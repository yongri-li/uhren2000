  --main_lh_h:          {{ settings.heading_line_height | divided_by: 100.0 }};

{% if settings.body_font == 'primary' %}
  --main_ff:            var(--main_ff_h);
  --main_fs:            var(--main_fs_h);
{% else %}
  --main_ff:            {{ settings.secondary_font.family | remove: '"' }}, {{ settings.secondary_font.fallback_families }};
  --main_fs:            {{ settings.secondary_font.style }};
{% endif %}
  --main_tt:            {{ settings.body_case }};
  --main_fw:            {{ settings.body_font_weight }};
  --main_fw_strong:     {{ settings.body_font_weight | plus: 300 | at_most: 700 }};

  --main_ls:            {% if settings.body_letter_spacing == '0' %}normal{% else %}{{ settings.body_letter_spacing | divided_by: 100.0 }}em{% endif %};
