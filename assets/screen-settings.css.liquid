  --main_h2:            {{ settings.h2_size }}px;
  --main_h3:            {{ settings.h3_size }}px;
  --main_h4:            {{ settings.h4_size }}px;
  --main_h_small:       {{ settings.h5_size }}px;

  --mob_h1:             {{ settings.h1_size_mobile }}px;
  --mob_h2:             {{ settings.h2_size_mobile }}px;
  --mob_h3:             {{ settings.h3_size_mobile }}px;
  --mob_h4:             {{ settings.h4_size_mobile }}px;
  --mob_h5:             {{ settings.h5_size_mobile }}px;
  --mob_h_small:        {{ settings.h5_size_mobile }}px;
  --mob_fz:             {{ settings.body_font_size_mobile }};
  --btn_fz_mob:         {{ settings.button_fontsize_mobile }};

{% if settings.button_font == 'primary' %}
  --btn_ff:            var(--main_ff_h);
  --btn_fs:            var(--main_fs_h);
{% else %}
  --btn_ff:            {{ settings.secondary_font.family | remove: '"' }}, {{ settings.secondary_font.fallback_families }};
  --btn_fs:            {{ settings.secondary_font.style }};
{% endif %}
  --btn_fw:            {{ settings.button_font_weight }};
  --btn_fz:          {{ settings.button_fontsize }};
  --btn_tt:          {{ settings.button_case }};
  --btn_ls:          {% if settings.button_letter_spacing == '0' %}normal{% else %}{{ settings.button_letter_spacing | divided_by: 100.0 }}em{% endif %};
  --btn_bd: {{ settings.button_border }}px;
  --btn_br: {{ settings.button_radius }}px;
  --btn_pv: {{ settings.button_padding_vertical }}px;
  --btn_ph: {{ settings.button_padding_horizontal }}px;


{% if settings.prices_font == 'primary' %}
  --price_ff:            var(--main_ff_h);
  --price_fs:            var(--main_fs_h);
{% else %}
  --price_ff:            {{ settings.secondary_font.family | remove: '"' }}, {{ settings.secondary_font.fallback_families }};
  --price_fs:            {{ settings.secondary_font.style }};
{% endif %}
  --price_fw:            {{ settings.prices_font_weight }};

{% unless settings.everything_rounded %}
  --b2r: 0;
{% endunless %}
  --b2i: {{ settings.input_border_radius }}px;
  --b2p: {{ settings.images_border_radius }}px;

  --glw: {% if settings.width == 2000 %}10000{% else %}{{ settings.width }}{% endif %}px;
  --ratio: {{ settings.custom_product_image_ratio | divided_by: 100.0 }};
}
.l4pr { --ratio: {{ settings.custom_product_image_ratio | divided_by: 100.0 }}; }

{% if settings.width > 1600 %}
body.template-product {
  --glw: 1600px;
}
{% endif %}
@media only screen and (max-width: 760px) {
  :root {
    --main_mr: 20px;
    --size_70_f: 12vw;
    --main_fz: var(--mob_fz);
    --btn_fz: var(--btn_fz_mob);
