    "section_custom_liquid_9CBiL9": {
      "type": "section-custom-liquid",
      "custom_css": [
        ".alphabet-navigation {display: flex; justify-content: center; align-items: center;}"
      ],
      "settings": {
        "custom_liquid": "<style>\nbody {\n  margin: 0; \/* Remove default margin *\/\n  padding: 0; \/* Remove default padding *\/\n}\n\n.container {\n  display: flex;\n  justify-content: center; \/* Horizontally center the content *\/\n  width: 100%; \/* Ensure the container takes full width *\/\n}\n\n.content-wrapper {\n  display: flex; \/* Use flexbox to center children *\/\n  justify-content: center; \/* Horizontally center the content *\/\n  width: 100%; \/* Take up full width *\/\n}\n\n.alphabet-navigation {\n  \/* Now the navigation is centered within the content-wrapper *\/\n  display: flex;\n  justify-content: center;\n  padding: 10px 0; \/* Add some padding at the top and bottom *\/\n  flex-wrap: wrap; \/* Allow items to wrap to the next row *\/\n}\n\n\/* Styling for buttons with links *\/\n.alphabet-navigation span.button-link,\n.alphabet-navigation span.button-no-link {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  text-decoration: none;\n  background-color: #fff;\n  color: #B99E7A;\n  border: 2px solid #B99E7A;\n  border-radius: 50%;\n  width: 40px; \/* Width of the buttons *\/\n  height: 40px; \/* Height of the buttons *\/\n  margin: 5px;\n  cursor: pointer;\n  transition: border-color 0.3s, background-color 0.3s, color 0.3s;\n}\n\n\/* Hover effect for buttons with links *\/\n.alphabet-navigation span.button-link:hover {\n  background-color: black;\n  color: #fff;\n  border-color: black;\n}\n\n\/* Styling for buttons without links *\/\n.alphabet-navigation span.button-no-link {\n  background-color: white;\n  color: #333;\n  border: 2px solid #ccc;\n}\n<\/style>\n<\/head>\n<body>\n\n<div class=\"container\">\n  <!-- Content wrapper -->\n  <div class=\"content-wrapper\">\n    <!-- Alphabet navigation -->\n    <div class=\"alphabet-navigation\">\n      {% paginate blogs['lexikon'].articles by 500 %}\n        {% assign alphabet = \"abcdefghijklmnopqrstuvwxyz\" | split: \"\" %}\n        {% assign blog_posts = blogs['lexikon'].articles %}\n        {% assign indexed_posts = \"\" %}\n\n        <!-- Generate a list of starting letters of all blog posts -->\n        {% for post in blog_posts %}\n          {% assign first_letter = post.title | first | downcase %}\n          {% unless indexed_posts contains first_letter %}\n            {% assign indexed_posts = indexed_posts | append: first_letter %}\n          {% endunless %}\n        {% endfor %}\n\n        <!-- Alphabet links -->\n        {% for letter in alphabet %}\n          {% if indexed_posts contains letter %}\n            <!-- Span for buttons with link for each letter with posts -->\n            <span class=\"button-link\" onclick=\"window.location='\/pages\/lexikon-{{ letter | upcase }}'\">{{ letter | upcase }}<\/span>\n          {% else %}\n            <!-- Span for buttons without link for letters without posts -->\n            <span class=\"button-no-link\">{{ letter | upcase }}<\/span>\n          {% endif %}\n        {% endfor %}\n      {% endpaginate %}\n    <\/div>\n  <\/div>\n<\/div>\n\n<\/body>\n<\/html>",
        "spacing_desktop": 50,
        "spacing_mobile": 50,
        "fix_zindex": 0
      }
    },
