    "section_custom_liquid_9CBiL9": {
      "type": "section-custom-liquid",
      "custom_css": [
        "{max-width: 900px; text-align: center;}"
      ],
      "settings": {
        "custom_liquid": "<style>\n    .container {\n      display: flex;\n    }\n    .alphabet-navigation {\n      justify-content: center;\n    }\n  <\/style>\n<\/head>\n<body>\n\n<div class=\"container\">\n  <!-- Content wrapper -->\n  <div class=\"content-wrapper\">\n    <!-- Alphabet navigation -->\n    <div class=\"alphabet-navigation\">\n      {% assign alphabet = \"abcdefghijklmnopqrstuvwxyz\" | split: \"\" %}\n      {% assign blog_posts = blogs['default-blog'].articles %}\n      {% assign indexed_posts = \"\" %}\n\n      <!-- Generate a list of starting letters of all blog posts -->\n      {% for post in blog_posts %}\n        {% assign first_letter = post.title | first | downcase %}\n        {% unless indexed_posts contains first_letter %}\n          {% assign indexed_posts = indexed_posts | append: first_letter %}\n        {% endunless %}\n      {% endfor %}\n\n      <!-- Alphabet buttons -->\n      {% for letter in alphabet %}\n        {% if indexed_posts contains letter %}\n          <!-- Button for each letter with posts -->\n          <button onclick=\"location.href='\/blogs\/news\/{{ letter }}'\">{{ letter | upcase }}<\/button>\n        {% else %}\n          <!-- Disabled button for letters without posts -->\n          <button disabled>{{ letter | upcase }}<\/button>\n        {% endif %}\n      {% endfor %}\n    <\/div>\n  <\/div>\n<\/div>\n\n<\/body>",
        "spacing_desktop": 50,
        "spacing_mobile": 50,
        "fix_zindex": 0
      }
